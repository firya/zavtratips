{
  "name": "zavtratips",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:server": "tsc --project server/tsconfig.json",
    "type-check": "tsc",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "tsx server/index.ts",
    "server:dev": "nodemon --watch 'server/**/*.ts' --exec 'tsx' server/index.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run server:dev\"",
    "bot:dev": "npm run build && nodemon --watch 'server/**/*.ts' --exec 'tsx' server/index.ts",
    "sync-db": "tsx scripts/sync-sheets.ts",
    "db:show": "npx prisma studio",
    "production": "NODE_ENV=production npm run build && NODE_ENV=production tsx server/index.ts"
  },
  "dependencies": {

    "@types/localtunnel": "^2.0.4",
    "@types/node-cron": "^3.0.11",
    "@types/node-telegram-bot-api": "^0.64.8",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "googleapis": "^148.0.0",
    "localtunnel": "^2.0.2",
    "lucide-react": "^0.344.0",
